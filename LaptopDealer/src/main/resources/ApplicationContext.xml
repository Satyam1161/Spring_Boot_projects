<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- bean definitions here -->
    <bean id="I3" class="com.example.LaptopDealer.I3"></bean>
    <bean id="i5" class="com.example.LaptopDealer.i5"></bean>
    <bean id="i7" class="com.example.LaptopDealer.i7"></bean>

<bean id="Dellwithi3" class="com.example.LaptopDealer.Dell">
    <constructor-arg ref="I3"></constructor-arg>
</bean>
    <bean id="Dellwithi5" class="com.example.LaptopDealer.Dell">
        <constructor-arg ref="i5"></constructor-arg>
    </bean>
    <bean id="Dellwithi7" class="com.example.LaptopDealer.Dell">
        <constructor-arg ref="i7"></constructor-arg>
    </bean>
    <bean id="MacBookwithi3" class="com.example.LaptopDealer.MacBook">
        <constructor-arg ref="I3"></constructor-arg>
    </bean>
    <bean id="MacBookwithi5" class="com.example.LaptopDealer.MacBook">
        <constructor-arg ref="i5"></constructor-arg>
    </bean>
    <bean id="MacBookwithi7" class="com.example.LaptopDealer.MacBook">
        <constructor-arg ref="i7"></constructor-arg>
    </bean>
    <bean id="Microsofti3" class="com.example.LaptopDealer.Microsoft">
        <constructor-arg ref="I3"></constructor-arg>
    </bean>
    <bean id="Microsofti5" class="com.example.LaptopDealer.Microsoft">
        <constructor-arg ref="i5"></constructor-arg>
    </bean>
    <bean id="Microsofti7" class="com.example.LaptopDealer.Microsoft">
        <constructor-arg ref="i7"></constructor-arg>
    </bean>
</beans>